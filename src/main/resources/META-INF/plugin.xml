<idea-plugin version="2">
    <id>net.orekyuu.semicolonless</id>
    <name>Semicolon-less Java Support</name>
    <vendor email="orekyuu@gmail.com" url="http://orekyuu.net">orekyuu</vendor>

    <description><![CDATA[
      Semicolon-less Java Support.
    ]]></description>

    <change-notes><![CDATA[
      2016/ 9/30: 1.0.0
      2016/10/10: 1.0.1
    ]]>
    </change-notes>

    <idea-version since-build="162" until-build="162.*"/>

    <extensions defaultExtensionNs="com.intellij">
        <localInspection
                displayName="Unused semicolon"
                groupName="Semicolon-less Java"
                language="JAVA"
                enabledByDefault="false"
                level="ERROR"
                implementationClass="net.orekyuu.semicolonless.inspection.SemicolonUsedInspection"
                />
    </extensions>

    <actions>
    </actions>

</idea-plugin>